<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>i18n.messages</base-name>
            <var>msg</var>
        </resource-bundle>
        <locale-config>
            <default-locale>pl</default-locale>
            <supported-locale>en</supported-locale>
        </locale-config>
    </application>
    
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/main/index.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>createAccount</from-outcome>
            <to-view-id>/accounts/createAccount.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>employeeItemList</from-outcome>
            <to-view-id>/employeeItemList/employeeItemList.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>realizationError</from-outcome>
            <to-view-id>/error/realizationError.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>register</from-outcome>
            <to-view-id>/login/register.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/login/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>accountList</from-outcome>
            <to-view-id>/accounts/accounts.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>basket</from-outcome>
            <to-view-id>/basket/basket.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>orders</from-outcome>
            <to-view-id>/orders/orders.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>customSearch</from-outcome>
            <to-view-id>/customSearch/customSearch.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>addItemDescription</from-outcome>
            <to-view-id>/employeeItemList/createItemDescriptionForm.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>createItem</from-outcome>
            <to-view-id>/employeeItemList/createItemForm.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/orders/orders.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>orderRealization</from-outcome>
            <to-view-id>/orders/orderRealization.xhtml</to-view-id>
             <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/employeeItemList/createItemDescriptionForm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>createGenreForm</from-outcome>
            <to-view-id>/employeeItemList/createGenreForm.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>
